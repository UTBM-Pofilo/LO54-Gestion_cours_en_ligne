<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="fr.utbm.lo54.project.core.entity.Client" optimistic-lock="version" schema="LO54" table="CLIENT">
    <id name="id" type="int">
      <column name="ID"/>
      <generator class="increment"/>
    </id>
    <property name="lastName" type="string">
      <column length="14" name="LAST_NAME" not-null="true"/>
    </property>
    <property name="firstName" type="string">
      <column length="14" name="FIRST_NAME" not-null="true"/>
    </property>
    <property name="address" type="string">
      <column length="14" name="ADDRESS" not-null="true"/>
    </property>
    <property name="phone" type="string">
      <column length="10" name="PHONE" not-null="true"/>
    </property>
    <property name="email" type="string">
      <column length="14" name="EMAIL"/>
    </property>
    <property name="courseSessionId" type="java.lang.Integer">
      <column name="COURSE_SESSION_ID"/>
    </property>
  </class>
</hibernate-mapping>
